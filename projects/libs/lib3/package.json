{
  "name": "@libs/lib3",
  "version": "0.0.1",
  "main": "src/public-api",
  "peerDependencies": {
    "@angular/common": "^15.2.0",
    "@angular/core": "^15.2.0"
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@libs/lib1": "*"
  },
  "sideEffects": false,
  "scripts": {
    "build": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "command": "ng build lib3 --configuration production",
      "dependencies": [
        "../lib1:build"
      ],
      "files": [
        "src/**"
      ],
      "output": []
    },
    "test": {
      "command": "ng test lib3 --watch false",
      "files": [
        "src/**"
      ],
      "output": []
    }
  }
}